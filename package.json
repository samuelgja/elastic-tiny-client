{
  "name": "elastic-tiny-client",
  "version": "0.1.3",
  "main": "lib/index.js",
  "repository": "https://github.com/samuelgja/elastic-tiny-client",
  "scripts": {
    "build": "tsc && node scripts/copy-data.mjs",
    "watch": "nodemon --config nodemon.json",
    "watch:test": "nodemon --config nodemon.test.json",
    "lint": "npx eslint \"src/**\"",
    "format": "npx prettier --list-different --write \"src/**/*.{ts,tsx}\"",
    "code-check": "yarn typecheck && yarn lint && yarn format && bun test",
    "typecheck": "tsc --noEmit",
    "pub": "yarn code-check && yarn build",
    "test": "jest --env=jsdom --verbose --runInBand"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "bun-types": "^0.5.8",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-unicorn": "40.1.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
